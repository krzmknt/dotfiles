{
  "global": { "show_in_menu_bar": false },
  "profiles": [
    {
      "complex_modifications": {
        "rules": [
          {
            "description": "Ctrl+U to toggle WezTerm",
            "manipulators": [
              {
                "from": {
                  "key_code": "u",
                  "modifiers": {
                    "mandatory": ["control"],
                    "optional": ["caps_lock"]
                  }
                },
                "to": [
                  {
                    "shell_command": "/usr/bin/osascript -e 'tell application \"WezTerm\" to activate'"
                  }
                ],
                "type": "basic"
              }
            ]
          },
          {
            "description": "Ctrl+I to activate Google Chrome",
            "manipulators": [
              {
                "from": {
                  "key_code": "i",
                  "modifiers": {
                    "mandatory": ["control"],
                    "optional": ["caps_lock"]
                  }
                },
                "to": [
                  {
                    "shell_command": "/usr/bin/osascript -e 'tell application \"Google Chrome\" to activate'"
                  }
                ],
                "type": "basic"
              }
            ]
          },
          {
            "description": "Cmd+J/K to Shift+Left/Right (WezTerm)",
            "manipulators": [
              {
                "from": {
                  "key_code": "j",
                  "modifiers": {
                    "mandatory": ["command"],
                    "optional": ["caps_lock"]
                  }
                },
                "to": [{ "key_code": "left_arrow", "modifiers": ["shift"] }],
                "type": "basic",
                "conditions": [
                  {
                    "type": "frontmost_application_if",
                    "bundle_identifiers": ["^com\\.github\\.wez\\.wezterm$"]
                  }
                ]
              },
              {
                "from": {
                  "key_code": "k",
                  "modifiers": {
                    "mandatory": ["command"],
                    "optional": ["caps_lock"]
                  }
                },
                "to": [{ "key_code": "right_arrow", "modifiers": ["shift"] }],
                "type": "basic",
                "conditions": [
                  {
                    "type": "frontmost_application_if",
                    "bundle_identifiers": ["^com\\.github\\.wez\\.wezterm$"]
                  }
                ]
              }
            ]
          },
          {
            "description": "Cmd+J/K to switch tabs",
            "manipulators": [
              {
                "from": {
                  "key_code": "j",
                  "modifiers": {
                    "mandatory": ["command"],
                    "optional": ["caps_lock"]
                  }
                },
                "to": [
                  { "key_code": "tab", "modifiers": ["control", "shift"] }
                ],
                "type": "basic",
                "conditions": [
                  {
                    "type": "frontmost_application_unless",
                    "bundle_identifiers": ["^com\\.github\\.wez\\.wezterm$"]
                  }
                ]
              },
              {
                "from": {
                  "key_code": "k",
                  "modifiers": {
                    "mandatory": ["command"],
                    "optional": ["caps_lock"]
                  }
                },
                "to": [{ "key_code": "tab", "modifiers": ["control"] }],
                "type": "basic",
                "conditions": [
                  {
                    "type": "frontmost_application_unless",
                    "bundle_identifiers": ["^com\\.github\\.wez\\.wezterm$"]
                  }
                ]
              }
            ]
          },
          {
            "description": "Cmd+Shift+J/K to Back/Forward (Chrome)",
            "manipulators": [
              {
                "from": {
                  "key_code": "j",
                  "modifiers": {
                    "mandatory": ["command", "shift"],
                    "optional": ["caps_lock"]
                  }
                },
                "to": [
                  { "key_code": "open_bracket", "modifiers": ["command"] }
                ],
                "type": "basic",
                "conditions": [
                  {
                    "type": "frontmost_application_if",
                    "bundle_identifiers": ["^com\\.google\\.Chrome$"]
                  }
                ]
              },
              {
                "from": {
                  "key_code": "k",
                  "modifiers": {
                    "mandatory": ["command", "shift"],
                    "optional": ["caps_lock"]
                  }
                },
                "to": [
                  { "key_code": "close_bracket", "modifiers": ["command"] }
                ],
                "type": "basic",
                "conditions": [
                  {
                    "type": "frontmost_application_if",
                    "bundle_identifiers": ["^com\\.google\\.Chrome$"]
                  }
                ]
              }
            ]
          },
          {
            "description": "Ctrl+; to Return",
            "manipulators": [
              {
                "from": {
                  "key_code": "semicolon",
                  "modifiers": {
                    "mandatory": ["control"],
                    "optional": ["caps_lock"]
                  }
                },
                "to": [{ "key_code": "return_or_enter" }],
                "type": "basic"
              }
            ]
          },
          {
            "description": "Ctrl+Shift+HJKL to Arrow Keys",
            "manipulators": [
              {
                "from": {
                  "key_code": "h",
                  "modifiers": { "mandatory": ["control", "shift"] }
                },
                "to": [{ "key_code": "left_arrow" }],
                "type": "basic"
              },
              {
                "from": {
                  "key_code": "j",
                  "modifiers": { "mandatory": ["control", "shift"] }
                },
                "to": [{ "key_code": "down_arrow" }],
                "type": "basic"
              },
              {
                "from": {
                  "key_code": "k",
                  "modifiers": { "mandatory": ["control", "shift"] }
                },
                "to": [{ "key_code": "up_arrow" }],
                "type": "basic"
              },
              {
                "from": {
                  "key_code": "l",
                  "modifiers": { "mandatory": ["control", "shift"] }
                },
                "to": [{ "key_code": "right_arrow" }],
                "type": "basic"
              }
            ]
          },
          {
            "description": "コマンドキーを単体で押したときに、英数・かなキーを送信する。（左コマンドキーは英数、右コマンドキーはかな） (rev 3)",
            "manipulators": [
              {
                "from": {
                  "key_code": "left_command",
                  "modifiers": { "optional": ["any"] }
                },
                "parameters": {
                  "basic.to_if_held_down_threshold_milliseconds": 100
                },
                "to": [
                  {
                    "key_code": "left_command",
                    "lazy": true
                  }
                ],
                "to_if_alone": [{ "key_code": "japanese_eisuu" }],
                "to_if_held_down": [{ "key_code": "left_command" }],
                "type": "basic"
              },
              {
                "from": {
                  "key_code": "right_command",
                  "modifiers": { "optional": ["any"] }
                },
                "parameters": {
                  "basic.to_if_held_down_threshold_milliseconds": 100
                },
                "to": [
                  {
                    "key_code": "right_command",
                    "lazy": true
                  }
                ],
                "to_if_alone": [{ "key_code": "japanese_kana" }],
                "to_if_held_down": [{ "key_code": "right_command" }],
                "type": "basic"
              }
            ]
          },
          {
